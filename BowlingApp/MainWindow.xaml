<Window x:Name="___No_Name_" x:Class="BowlingApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BowlingApp"
        mc:Ignorable="d"
        Title="Bowling" Height="600" Width="1200" ResizeMode="NoResize">
    <Grid>
        <Grid Margin="10,10,10,10">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <local:BowlerFrames x:Name="firstBowlerFrame" Grid.Row="0"/>
            <local:BowlerFrames x:Name="secondBowlerFrame" Grid.Row="1"/>
            <local:BowlerFrames x:Name="thirdBowlerFrame" Grid.Row="2"/>
            <local:BowlerFrames x:Name="fourthBowlerFrame" Grid.Row="3"/>
        </Grid>
    </Grid>
</Window>
